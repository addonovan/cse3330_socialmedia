{% extends "../base" %}

{% block title %}
    {{user.fullName}} | SocialMediaSite
{% endblock %}

{% block content %}
    {% include "../post_box" with {"allowEvents": "true", "wall": user} %}

    <div id="Overview">
        {% for post in overview %}
            {% include "../post" with {"wall": user} %}
        {% endfor %}
    </div>
{% endblock %}
